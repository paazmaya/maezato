{
  "name": "maezato",
  "version": "0.12.0",
  "description": "Clone all repositories of a given user at GitHub",
  "homepage": "https://github.com/paazmaya/maezato",
  "bin": "bin/maezato.js",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "c8 --all --exclude tests tape tests/*_test.js tests/lib/*_test.js",
    "coverage": "c8 --all --exclude tests report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "eslint *.js bin lib"
  },
  "files": [
    "bin",
    "lib",
    "index.js",
    "npm-shrinkwrap.json",
    "package.json",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.15.0",
    "npm": ">=6.14.8"
  },
  "author": {
    "name": "Juga Paazmaya",
    "email": "paazmaya@yahoo.com",
    "url": "https://paazmaya.fi"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:paazmaya/maezato.git"
  },
  "bugs": {
    "url": "https://github.com/paazmaya/maezato/issues"
  },
  "license": "MIT",
  "dependencies": {
    "@octokit/graphql": "5.0.5",
    "got": "12.6.0",
    "mkdirp": "1.0.4",
    "optionator": "0.9.1",
    "progress": "2.0.3",
    "promise-each": "2.2.0"
  },
  "devDependencies": {
    "codecov": "3.8.3",
    "eslint": "8.40.0",
    "eslint-config-paazmaya": "9.0.1",
    "nock": "13.3.1",
    "c8": "7.13.0",
    "tape": "5.6.3"
  }
}
