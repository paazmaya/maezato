{
  "name": "maezato",
  "version": "0.13.0",
  "description": "Clone all repositories of a given user at GitHub",
  "homepage": "https://github.com/paazmaya/maezato",
  "bin": "bin/maezato.js",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "c8 --exclude tests tape tests/*_test.js tests/lib/*_test.js",
    "coverage": "c8 --exclude tests report --reporter=text-lcov > coverage.lcov",
    "lint": "eslint *.js bin lib"
  },
  "files": [
    "bin",
    "lib",
    "index.js",
    "npm-shrinkwrap.json",
    "package.json",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.12.0",
    "npm": ">=8.19.2"
  },
  "author": {
    "name": "Juga Paazmaya",
    "email": "paazmaya@yahoo.com",
    "url": "https://paazmaya.fi"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:paazmaya/maezato.git"
  },
  "bugs": {
    "url": "https://github.com/paazmaya/maezato/issues"
  },
  "license": "MIT",
  "dependencies": {
    "@octokit/graphql": "7.0.1",
    "got": "13.0.0",
    "mkdirp": "3.0.1",
    "optionator": "0.9.3",
    "progress": "2.0.3",
    "promise-each": "2.2.0"
  },
  "devDependencies": {
    "c8": "8.0.0",
    "eslint": "8.44.0",
    "eslint-config-paazmaya": "9.0.1",
    "msw": "1.2.2",
    "tape": "5.6.4"
  }
}
